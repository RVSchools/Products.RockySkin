<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="Products.RockySkin"
    purge="False">

    <configlet
        title="RVS Icon Bar"
        action_id="iconbar"
        appId="Products.RockySkin"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@iconbar-configuration"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
